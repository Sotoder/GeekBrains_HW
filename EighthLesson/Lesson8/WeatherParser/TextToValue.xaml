<Window x:Class="WeatherParser.TextToValue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WeatherParser"
        mc:Ignorable="d"
        Title="TextToValue" Height="267" Width="252">
    <Grid Margin="0,0,0,2">
        <TextBox Name ="tbDate" TextAlignment="Center" HorizontalAlignment="Center" Margin="0,11,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyDown="tbDate_KeyDown"/>
        <Calendar Name="calendar" HorizontalAlignment="Center" VerticalAlignment="Center" SelectedDatesChanged="calendar_SelectedDatesChanged"/>
    </Grid>
</Window>
